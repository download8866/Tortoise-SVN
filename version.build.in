<?xml version="1.0"?>
<project name="SetVersionInfo" default="setversioninfo">
  <target name="setversioninfo">
    <setenv>
      <variable name="MajorVersion" value="1" />
      <variable name="MinorVersion" value="10" />
      <variable name="MicroVersion" value="3" />
      <variable name="WCREV" value="$WCREV$" />
      <variable name="WCDATE" value="$WCDATE$" />
      <variable name="SVNMajorVersion" value="1" />
      <variable name="SVNMinorVersion" value="10" />
      <variable name="SVNMicroVersion" value="4" />
    </setenv>
  </target>
</project>